{
    "name": "Process Engine",
    "type": "project",
    "description": "Process Engine example using vespolina workflow",
    "keywords": [
        "workflow", "design"
    ],
    "homepage": "http://vespolina.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "vespolina/workflow": "dev-master",
        "monolog/monolog": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "raulfraile/ladybug": "~1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ProcessEngine\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}